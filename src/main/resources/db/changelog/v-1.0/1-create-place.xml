<?xml version="1.1" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.2.xsd
        http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.2.xsd">

    <changeSet  id="2"  author="anton">
        <createTable tableName="place">
            <column  name="id"  type="bigint">
                <constraints  primaryKey="true"  nullable="false"/>
            </column>
            <column name="count" type="bigint" defaultValue="0">
                <constraints nullable="false" />
            </column>
            <column name="description" type="varchar(256)">
                <constraints nullable="false"/>
            </column>
            <column name="full_name" type="varchar(256)">
                <constraints nullable="false" unique="true" checkConstraint="[0..10]"/>
            </column>
            <column name="short_name" type="varchar(256)">
                <constraints nullable="false" unique="true"/>
            </column>
            <column name="rating" type="double">
                <constraints/>
            </column>
            <column name="type_place" type="bigint">

            </column>
        </createTable>

    </changeSet>




</databaseChangeLog>